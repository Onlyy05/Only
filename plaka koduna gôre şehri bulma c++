#include <iostream>
using namespace std;

int main()
{
    int plaka;
    cout << "plaka kodu giriniz: ";
    cin >> plaka;

    switch (plaka)
    {
    case 1:
        cout << "Adana" << endl;
        break;
    case 6:
        cout << "Ankara" << endl;
        break;
    case 7:
        cout << "Antalya" << endl;
        break;
    case 16:
        cout << "Bursa" << endl;
        break;
    case 34:
        cout << "Istanbul" << endl;
        break;
    case 35:
        cout << "Izmir" << endl;
        break;
    default:
        cout << "Girdiginiz plaka kodu listede bulunamadi." << endl;
        break;
    }

    return 0;
}
